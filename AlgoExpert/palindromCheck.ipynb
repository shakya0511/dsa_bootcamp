    "def isPalindrome(string):\n",
    "    # Write your code here.\n",
    "    leftIdx = 0\n",
    "    rightIdx = len(string)-1\n",
    "\n",
    "    while leftIdx < rightIdx:\n",
    "        if string[leftIdx] != string[rightIdx]:\n",
    "            return False\n",
    "\n",
    "        leftIdx += 1\n",
    "        rightIdx -= 1\n",
    "        \n",
    "    return True"
   ]
